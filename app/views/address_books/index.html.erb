<p id="notice"><%= notice %></p>

<h1>Address Books</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Zipcode</th>
      <th>Zipcode 01</th>
      <th>Zipcode 02</th>
      <th>Address 01</th>
      <th>Address 02</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @address_books.each do |address_book| %>
      <tr>
        <td><%= address_book.name %></td>
        <td><%= address_book.phone %></td>
        <td><%= address_book.zipcode %></td>
        <td><%= address_book.zipcode_01 %></td>
        <td><%= address_book.zipcode_02 %></td>
        <td><%= address_book.address_01 %></td>
        <td><%= address_book.address_02 %></td>
        <td><%= link_to 'Show', address_book %></td>
        <td><%= link_to 'Edit', edit_address_book_path(address_book) %></td>
        <td><%= link_to 'Destroy', address_book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Address Book', new_address_book_path %>
